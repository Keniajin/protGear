Package: protGear
Type: Package
Title: Protein Micro Array Data Management and Interactive Visualization
Version: 1.3.1
Authors@R: c(person("Kennedy", "Mwai", role = c("cre", "aut"), email = "keniajin@gmail.com"),
             person(c("James", "Mburu"), role = "aut" , email = "mburuwanja@gmail.com") ,
